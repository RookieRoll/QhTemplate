@model QhTemplate.AdminWeb.ViewModels.Organizations.AddUsersToOrganizationViewModel

<div class="modal fade small" id="my-modal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">添加成员——@Model.Name</h4>
            </div>
            <div class="modal-body">
                <table id="add-user-table" class="table table-striped table-bordered user-table">

                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal" id="submit-user">确定</button>
                <button type="button" class="btn btn-default" data-dismiss="modal" id="cancel">取消</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<script>
    $("#my-modal").modal('show');
</script>

